{
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -g -Wall -Wextra -Wshadow -Wfloat-equal -pedantic -std=c++17 -O2 -Wformat=2 -Wconversion -Wno-sign-conversion -lm $fileName -o $fileNameWithoutExt.out && ./$fileNameWithoutExt.out"
    },
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, ColumnLimit: 0 }"
}